			factoryClass="org.opentravel.schemas.navigation.NavigationServiceFactory">
			<service serviceClass="org.opentravel.schemas.navigation.INavigationService">
			</service>
		</serviceFactory>
		<sourceProvider
			provider="org.opentravel.schemas.navigation.GlobalSelectionProvider">
			<variable name="org.opentravel.schemas.selection.nodes"
				priorityLevel="workbench">
			</variable>
			<variable name="org.opentravel.schemas.selection.navigationView"
				priorityLevel="workbench">
			</variable>
			<variable name="org.opentravel.schemas.selection.typeView"
				priorityLevel="workbench">
			</variable>
		</sourceProvider>
	</extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="oneElementSelected">
			<with variable="selection">
				<count value="1">
				</count>
			</with>
		</definition>
	</extension>
	<extension point="org.eclipse.ui.decorators">
		<decorator class="org.opentravel.schemas.views.decoration.LibraryDecorator"
			id="org.opentravel.schemas.stl2Developer.decorator.library" label="Library Decoration"
			lightweight="true" state="true">
		</decorator>
	</extension>
</plugin>
