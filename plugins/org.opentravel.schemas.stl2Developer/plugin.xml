	<extension point="org.eclipse.ui.handlers">
		<handler class="org.opentravel.schemas.commands.NavigationCommand:BACKWARD"
			commandId="stl2Developer.navigation.backward">
			<enabledWhen>
				<or>
					<not>
						<test property="stl2Developer.navigation.tester.backwardIsEmpty">
						</test>
					</not>
				</or>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.NavigationCommand:FORWARD"
			commandId="stl2Developer.navigation.forward">
			<enabledWhen>
				<or>
					<not>
						<test property="stl2Developer.navigation.tester.forwardIsEmpty">
						</test>
					</not>
				</or>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.NewRepository"
			commandId="org.opentravel.schemas.commands.repository.new">
		</handler>
		<handler class="org.opentravel.schemas.commands.RepositoryChangeCredentials"
			commandId="org.opentravel.schemas.commands.repository.update">
			<enabledWhen>
				<and>
					<reference definitionId="oneElementSelected">
					</reference>
					<with variable="selection">
						<iterate operator="and">
							<test property="stl2Developer.repository.type.remote">
							</test>
						</iterate>
					</with>
				</and>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.GoToTypeHandler"
			commandId="org.opentravel.schemas.commands.goto.type">
			<enabledWhen>
				<and>
					<reference definitionId="oneElementSelected">
					</reference>
					<with variable="selection">
						<iterate>
							<or>
								<test property="stl2Developer.node.hasType">
								</test>
								<instanceof value="org.opentravel.schemas.types.TypeNode">
								</instanceof>
							</or>
						</iterate>
					</with>
				</and>
			</enabledWhen>
			<activeWhen>
				<or>
					<with variable="activePartId">
						<equals value="org.opentravel.schemas.stl2Developer.NavigatorView">
						</equals>
					</with>
					<with variable="activePartId">
						<equals value="org.opentravel.schemas.stl2Developer.TypeView">
						</equals>
					</with>
				</or>
			</activeWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.SortNodeHandler"
			commandId="org.opentravel.schemas.commands.Sort">
			<enabledWhen>
				<with variable="org.opentravel.schemas.selection.nodes">
					<iterate ifEmpty="false" operator="or">
						<test property="stl2Developer.selection.newToChain">
						</test>
					</iterate>
				</with>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.AddNodeHandler2"
			commandId="org.opentravel.schemas.commands.Add">
			<enabledWhen>
				<with variable="org.opentravel.schemas.selection.nodes">
					<iterate ifEmpty="false" operator="or">
						<test property="stl2Developer.selection.canAdd">
						</test>
					</iterate>
				</with>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.AddNodeForNavigatorPopup"
			commandId="org.opentravel.schemas.commands.Add">
			<enabledWhen>
				<with variable="org.opentravel.schemas.selection.navigationView">
					<iterate ifEmpty="false" operator="or">
						<test property="stl2Developer.selection.canAdd">
						</test>
					</iterate>
				</with>
			</enabledWhen>
			<activeWhen>
				<with variable="activeMenu">
					<iterate ifEmpty="false" operator="or">
						<equals value="org.opentravel.schemas.stl2Developer.NavigatorView">
						</equals>
					</iterate>
				</with>
			</activeWhen>
		</handler>
		<handler
			class="org.opentravel.schemas.commands.DeleteDocumentationNodeHandler"
			commandId="org.eclipse.ui.edit.delete">
			<activeWhen>
				<with variable="activePartId">
					<equals value="org.opentravel.schemas.stl2Developer.DocumentationView">
					</equals>
				</with>
			</activeWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.DeleteNodesHandler"
			commandId="org.eclipse.ui.edit.delete">
			<enabledWhen>
				<and>
					<with variable="org.opentravel.schemas.selection.nodes">
						<iterate ifEmpty="false" operator="and">
							<and>
								<not>
									<instanceof value="org.opentravel.schemas.node.LibraryNode">
									</instanceof>
								</not>
								<not>
									<instanceof value="org.opentravel.schemas.node.LibraryChainNode">
									</instanceof>
								</not>
								<test property="stl2Developer.node.isDeleteable">
								</test>
							</and>
						</iterate>
					</with>
				</and>
			</enabledWhen>
			<!--workaround to force this handler to be activated as default -->
			<activeWhen>
				<with variable="activeWorkbenchWindow.activePerspective">
					<equals value="org.opentravel.schemas.stl2Developer.perspective">
					</equals>
				</with>
			</activeWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.RemoveRepository"
			commandId="org.eclipse.ui.edit.delete">
			<activeWhen>
				<with variable="activePartId">
					<equals value="org.opentravel.schemas.stl2Developer.RepositoryView">
					</equals>
				</with>
			</activeWhen>
			<enabledWhen>
				<and>
					<reference definitionId="oneElementSelected">
					</reference>
					<with variable="selection">
						<iterate operator="and">
							<test property="stl2Developer.repository.type.remote">
							</test>
						</iterate>
					</with>
				</and>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.NewComponentHandler"
			commandId="org.opentravel.schemas.commands.newComponent">
			<enabledWhen>
				<with variable="org.opentravel.schemas.selection.nodes">
					<iterate ifEmpty="false" operator="and">
						<and>
							<test property="stl2Developer.node.isInTLLibrary">
							</test>
							<test property="stl2Developer.node.isOwnerLibraryEditable">
							</test>
						</and>
					</iterate>
				</with>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.ResourceCommandHandler"
			commandId="org.opentravel.schemas.commands.newResource">
			<activeWhen>
				<with variable="activePartId">
					<equals value="org.opentravel.schemas.stl2Developer.ResourceView">
					</equals>
				</with>
			</activeWhen>
			<enabledWhen>
				<with variable="org.opentravel.schemas.selection.nodes">
					<iterate ifEmpty="false" operator="and">
						<and>
							<test property="stl2Developer.node.isInTLLibrary">
							</test>
							<test property="stl2Developer.node.isOwnerLibraryEditable">
							</test>
						</and>
					</iterate>
				</with>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.CreateProjectFromRepo"
			commandId="org.opentravel.schemas.commands.create.project">
			<activeWhen>
				<with variable="activePartId">
					<equals value="org.opentravel.schemas.stl2Developer.RepositoryView">
					</equals>
				</with>
			</activeWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.SaveLibraryHandler"
			commandId="org.opentravel.schemas.commands.SaveLibrary">
			<enabledWhen>
				<and>
					<with variable="org.opentravel.schemas.selection.nodes">
						<iterate ifEmpty="false" operator="and">
							<test property="stl2Developer.node.isOwnerLibraryEditable">
							</test>
						</iterate>
					</with>
				</and>
			</enabledWhen>
		</handler>
		<handler class="org.opentravel.schemas.commands.CompileHandler"
			commandId="org.opentravel.schemas.commands.Compile">
			<!-- enabledWhen>
				<and>
					<with variable="org.opentravel.schemas.selection.nodes">
						<iterate ifEmpty="false" operator="and">
							<test property="stl2Developer.node.isOwnerLibraryEditable">
							</test>
						</iterate>
					</with>
				</and>
			</enabledWhen -->
		</handler>
		<handler class="org.opentravel.schemas.commands.CloseProjectHandler"
			commandId="org.opentravel.schemas.commands.CloseProject">
		</handler>
		<handler class="org.opentravel.schemas.commands.ValidateHandler" 
			commandId="org.opentravel.schemas.commands.Validate">
		</handler>
		<handler class="org.opentravel.schemas.commands.CloseLibrariesHandler" 
