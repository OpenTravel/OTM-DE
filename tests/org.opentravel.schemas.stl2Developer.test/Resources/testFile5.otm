            <Attribute name="countryAccessCode" type="Telephone_CountryAccessCode">
                <Documentation>
                    <Description>Code assigned by telecommunications authorities for international country access identifier.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}NumericStringLength1to3</Implementer>
                </Documentation>
                <Equivalent context="Common">CountryAccessCode</Equivalent>
            </Attribute>
            <Attribute name="areaCityCode" type="Telephone_AreaCityCode">
                <Documentation>
                    <Description>Code assigned for telephones in a specific region, city, or area.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}NumericStringLength1to8</Implementer>
                </Documentation>
                <Equivalent context="Common">AreaCityCode</Equivalent>
            </Attribute>
            <Attribute mandatory="true" name="phoneNumber" type="Telephone_Number">
                <Documentation>
                    <Description>Telephone number assigned to a single location.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</Implementer>
                </Documentation>
                <Equivalent context="Common">PhoneNumber</Equivalent>
            </Attribute>
            <Attribute name="extension" type="Telephone_Extension">
                <Documentation>
                    <Description>Extension to reach a specific party at the phone number.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}NumericStringLength1to5</Implementer>
                </Documentation>
                <Equivalent context="Common">Extension</Equivalent>
            </Attribute>
            <Attribute name="id" type="xsd:ID">
                <Documentation>
                    <Description>Used elsewhere in the message to reference a specific telephone number (including faxes).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}RPH_Type</Implementer>
                </Documentation>
                <Equivalent context="Common">RPH</Equivalent>
            </Attribute>
        </Summary>
        <Detail>
            <Attribute name="pIN" type="String_Tiny">
                <Documentation>
                    <Description>Additional codes used for pager or telephone access rights.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to8</Implementer>
                </Documentation>
                <Equivalent context="Common">PIN</Equivalent>
            </Attribute>
            <Attribute name="remark" type="String_Text">
                <Documentation>
                    <Description>A remark associated with the telephone number.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</Implementer>
                </Documentation>
                <Equivalent context="Common">Remark</Equivalent>
            </Attribute>
            <Element name="Privacy" repeat="0" type="Privacy"/>
            <Element name="PhoneLocationType" repeat="0" type="PhoneLocation_Enum">
                <Documentation>
                    <Description>Describes the location of the phone, such as Home, Office, Property Reservation Office, etc.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</Implementer>
                </Documentation>
                <Equivalent context="Common">PhoneLocationType</Equivalent>
            </Element>
            <Element name="PhoneTechType" repeat="0" type="PhoneTechType_Enum">
                <Documentation>
                    <Description>Indicates type of technology associated with this telephone number, such as Voice, Data, Fax, Pager, Mobile, TTY, etc.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</Implementer>
                </Documentation>
                <Equivalent context="Common">PhoneTechType</Equivalent>
            </Element>
            <Element name="PhoneUseType" repeat="0" type="PhoneUseType_Enum">
                <Documentation>
                    <Description>Describes the type of telephone number, in the context of its general use (e.g. Home, Business, Emergency Contact, Travel Arranger, Day, Evening). Refer to OpenTravel Code List Phone Use Type (PUT).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</Implementer>
                </Documentation>
                <Equivalent context="Common">PhoneUseType</Equivalent>
            </Element>
            <Indicator name="defaultInd" publishAsElement="false">
                <Documentation>
                    <Description>When true, indicates a default value should be used.</Description>
                    <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}boolean</Implementer>
                </Documentation>
            </Indicator>
        </Detail>
        <Roles>
            <Role value="Home"/>
            <Role value="Office"/>
            <Role value="Mobile"/>
            <Role value="Other"/>
        </Roles>
    </CoreObject>
    
