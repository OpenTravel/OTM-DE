<?xml version="1.0" encoding="UTF-8"?>
<Library xmlns="http://www.OpenTravel.org/ns/OTA2/LibraryModel_v01_05"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.OpenTravel.org/ns/OTA2/LibraryModel_v01_05 OTA2_LibraryModel_v1.5.0.xsd">
    
    <VersionScheme>OTA2</VersionScheme>
    <Status>Draft</Status>
    <Namespace>http://www.opentravel.org/Sandbox/junits/v1</Namespace>
    <Prefix>tga</Prefix>
    <Name>Test5</Name>
    <Import namespace="http://www.w3.org/2001/XMLSchema" prefix="xsd"/>
    <Import namespace="http://www.opentravel.org/OTM/Common/v0" prefix="ota2"/>
    <Import fileHints="testFile1.otm"
        namespace="http://www.opentravel.org/Sandbox/junits/ns1/v1" prefix="tga"/>
    <Import fileHints="testFile4.otm"
        namespace="http://www.opentravel.org/Sandbox/junits/ns4/v4" prefix="t4"/>
    <Context
        applicationContext="http://www.opentravel.org/ns/OTA2/Demo/Profile/v01" context="Common"/>
    
    <Simple name="String_PersonName" type="String_Short">
        <Example context="Common">Sally Smythe</Example>
    </Simple>
    
    <Simple name="Flight_Number" pattern="[0-9]{1,4}[A-Z]?" type="xsd:string">
        <Equivalent context="Common">FlightNumber</Equivalent>
        <Example context="Common">1234A</Example>
    </Simple>
    
    <Simple name="Lniata" pattern="[A-F0-9]+" type="xsd:string">
        <Equivalent context="Common">Lniata</Equivalent>
        <Example context="Common">ABC123</Example>
    </Simple>
    
    <Simple maxLength="16" minLength="1" name="AgentSine" type="xsd:string">
        <Documentation>
            <Description>Identifies the agent or party within a business entity.</Description>
        </Documentation>
        <Equivalent context="Common">AgentSine</Equivalent>
        <Example context="Common">2345AB</Example>
    </Simple>
    
    <Simple name="Date_MMYY" pattern="(0[1-9]|1[0-2])[0-9][0-9]" type="xsd:string">
        <Documentation>
            <Description>Added this description and the a-z regex</Description>
        </Documentation>
        <Equivalent context="Common">TEST</Equivalent>
        <Example context="Common">0711</Example>
    </Simple>
    
    <ValueWithAttrs name="Amount" type="xsd:integer">
        <Example context="Common">2345.56</Example>
        <Attribute name="currencyCode" type="Currency_Code">
            <Documentation>
                <Description>soo</Description>
            </Documentation>
            <Example context="Common">USD</Example>
        </Attribute>
        <Attribute name="decimalPlaces" type="xsd:decimal">
            <Example context="Common">2</Example>
        </Attribute>
    </ValueWithAttrs>
    
    <Simple maxLength="128" name="String_Short" type="xsd:string"/>
    
    <Simple maxLength="512" name="String_Medium" type="xsd:string">
        <Equivalent context="Common"/>
    </Simple>
    
    <Simple maxLength="4096" name="String_Long" type="xsd:string"/>
    
    <Simple name="PositiveValueUnder1M" type="xsd:positiveInteger"/>
    
    <Simple name="PaymentCard_Issuer" type="String_Short">
        <Example context="Common">First Bank of Travel</Example>
    </Simple>
    
    <Simple name="PaymentCard_MagneticStrip" pattern="[0-9\-]{1,65}" type="String_Short">
        <Documentation>
            <Description>Payment card stripe.</Description>
            <Implementer>MICR characters are printed in the form of either an E-13B or CMC-7 Font.</Implementer>
            <MoreInfo>http://www4.infoprintsolutionscompany.com/help/index.jsp?topic=%2Fcom.ibm.printers.plngcfgguide%2Fav1i0mst105.htm</MoreInfo>
        </Documentation>
        <Example context="Common">111-111-111-111</Example>
    </Simple>
    
    <Simple name="Check_MICR" pattern="[0-9,: = ?]{1,38}" type="String_Short">
        <Documentation>
            <Description>Check MICR magnetic stripe.</Description>
            <Implementer>MICR characters are printed in the form of either an E-13B or CMC-7 Font.</Implementer>
            <MoreInfo>http://www4.infoprintsolutionscompany.com/help/index.jsp?topic=%2Fcom.ibm.printers.plngcfgguide%2Fav1i0mst105.htm</MoreInfo>
        </Documentation>
        <Example context="Common">00=344:333443445</Example>
    </Simple>
    
    <Simple name="PaymentCard_Type" type="String_Short">
        <Example context="Common">VVV</Example>
    </Simple>
    
    <Simple name="Usage" type="String_Short">
        <Example context="Common">usage</Example>
    </Simple>
    
    <Simple name="warning" type="String_Short">
        <Example context="Common">warning</Example>
    </Simple>
    
    <ValueWithAttrs name="Tax" type="xsd:positiveInteger">
        <Attribute name="amount" type="PositiveValueUnder1M"/>
        <Attribute name="taxCode" type="xsd:string"/>
        <Attribute name="taxName" type="xsd:string"/>
        <Attribute mandatory="true" name="ticketingCode" type="xsd:string">
            <Documentation>
                <Description>Required tax code used by ticketing</Description>
            </Documentation>
            <Equivalent context="Common">xxx33</Equivalent>
            <Example context="Common">R33</Example>
        </Attribute>
    </ValueWithAttrs>
    
    <ValueWithAttrs name="Location" type="String_Short">
        <Documentation>
            <Description>On location VWA</Description>
        </Documentation>
        <Example context="Common">DEN</Example>
        <Attribute name="codeContext" type="xsd:string">
            <Documentation>
                <Description>Whose code type</Description>
            </Documentation>
            <Example context="Common">IATA</Example>
        </Attribute>
        <Indicator name="multiAirportInd" publishAsElement="false">
            <Documentation>
                <Description>are there more than one airports served?</Description>
            </Documentation>
        </Indicator>
        <Indicator name="alternateLocationInd" publishAsElement="false">
            <Documentation>
                <Description>are others OK?</Description>
            </Documentation>
        </Indicator>
    </ValueWithAttrs>
    
    <CoreObject name="Employment" notExtendable="false">
        <Aliases/>
        <Simple type="String_Medium"/>
        <Summary>
            <Attribute name="number" type="String_Short">
                <Example context="Common">332258</Example>
            </Attribute>
            <Attribute name="title" type="String_Short">
                <Example context="Common">Geek-N-Nerd</Example>
            </Attribute>
            <Attribute name="level" type="String_Short">
                <Example context="Common">Director</Example>
            </Attribute>
            <Attribute name="status" type="Status_Employment">
                <Example context="Common">Active</Example>
            </Attribute>
            <Element name="Company" repeat="0" type="String_Short">
                <Example context="Common">Sabre, Inc.</Example>
            </Element>
            <Element name="Phones" repeat="0" type="Phones"/>
        </Summary>
        <Detail/>
        <Roles>
            <Role value="Current"/>
            <Role value="Previous"/>
        </Roles>
    </CoreObject>
    
    <Simple name="Status_Employment" type="String_Long">
        <Example context="Common">World Wide Air</Example>
    </Simple>
    
    <Simple name="PaymentCard_Number" pattern="[0-9]{1,19}" type="xsd:string">
        <Documentation>
            <Description/>
            <Reference>http://en.wikipedia.org/wiki/Credit_card_number</Reference>
        </Documentation>
        <Example context="Common">11111111111111</Example>
    </Simple>
    
    <Enumeration_Open name="HotelTypes">
        <Value label="" literal="motel"/>
        <Value label="" literal="hotel"/>
    </Enumeration_Open>
    
    <ValueWithAttrs name="Payment" type="ota2:Empty">
        <Documentation>
            <Description>Amount of the payment.</Description>
        </Documentation>
        <Example context="Common">23.56</Example>
        <Attribute name="tax" type="Money">
            <Documentation>
                <Description>Tax portion of the payment amount.</Description>
            </Documentation>
            <Example context="Common">0.67</Example>
        </Attribute>
        <Attribute name="PaymentCard_Simple" type="xsd:string">
            <Documentation>
                <Description>Payment card used to make payment.</Description>
            </Documentation>
            <Example context="Common">11136982222</Example>
        </Attribute>
        <Indicator name="idCheckedInd" publishAsElement="false">
            <Documentation>
                <Description>True if identification was checked prior to accepting payment.</Description>
            </Documentation>
        </Indicator>
    </ValueWithAttrs>
    
    <BusinessObject name="Profile" notExtendable="false">
        <Aliases>TravelerProfile</Aliases>
        <ID>
            <Attribute mandatory="true" name="authority" type="xsd:string">
                <Equivalent context="Common"/>
                <Example context="Common">Auth1</Example>
            </Attribute>
            <Element mandatory="true" name="ProfileID" repeat="0" type="xsd:string">
                <Equivalent context="Common"/>
                <Example context="Common">Profile123</Example>
            </Element>
        </ID>
        <Summary>
            <Element name="Name" repeat="0" type="String_PersonName"/>
            <Element name="Employment" repeat="0" type="Employment"/>
            <Element name="PaymentCardDetail" repeat="0" type="PaymentCard_Detail"/>
            <Element name="HomePhone" repeat="0" type="HomePhone"/>
            <Element name="Phone" repeat="0" type="Phone"/>
        </Summary>
        <Detail>
            <Element name="Remarks" repeat="0" type="String_Long"/>
            <Indicator name="shareInd" publishAsElement="false"/>
            <Indicator name="syncInd" publishAsElement="false"/>
            <Indicator name="holdInd" publishAsElement="false"/>
        </Detail>
        <Query context="Common" label="Profile" notExtendable="true">
            <Element name="PhoneDetail" repeat="0" type="Phone_Detail_List"/>
        </Query>
        <Custom context="Common" label="Legacy" notExtendable="false">
            <Element name="Remarks" repeat="0" type="String_Long">
                <Example context="Common">Remarks</Example>
            </Element>
            <Indicator name="shareInd" publishAsElement="false"/>
        </Custom>
    </BusinessObject>
    
    <ValueWithAttrs name="Age" type="xsd:integer">
        <Equivalent context="Common">Age</Equivalent>
        <Example context="Common">25</Example>
        <Attribute name="birthDate" type="xsd:date">
            <Equivalent context="Common">birthDate</Equivalent>
            <Example context="Common">1987-05-22</Example>
        </Attribute>
    </ValueWithAttrs>
    
    <ValueWithAttrs name="POS_Location" type="ota2:Empty">
        <Documentation>
            <Description>A location for an airline or agency agent. Can be used to describe the actual, pseudo or home
                location.</Description>
        </Documentation>
        <Equivalent context="Common">Location.POS</Equivalent>
        <Example context="Common">Example of VWA Value</Example>
        <Attribute name="country" type="String_Short">
            <Equivalent context="Common">country</Equivalent>
            <Example context="Common">USA</Example>
        </Attribute>
        <Attribute name="province" type="String_Short">
            <Equivalent context="Common">province</Equivalent>
            <Example context="Common">CA</Example>
        </Attribute>
        <Attribute name="city" type="String_Short">
            <Example context="Common">Den</Example>
        </Attribute>
        <Attribute name="number" type="xsd:int">
            <Documentation>
                <Description>agent station number or agency iata number</Description>
            </Documentation>
            <Equivalent context="Common">number</Equivalent>
            <Example context="Common">3</Example>
        </Attribute>
    </ValueWithAttrs>
    
    <BusinessObject name="POS" notExtendable="true">
        <Documentation>
            <Description>Locations: Psuedo - Agency PCC or Agent AAA Actual ? where the transaction is initiated Home ? assigned to the agent account</Description>
        </Documentation>
        <Aliases/>
        <ID>
            <Attribute name="pcc" type="String_Short">
                <Documentation>
                    <Description>Psuedo is either Agency PCC or Agent AAA. Required for most sources, unless MultiHost code is provided.</Description>
                </Documentation>
                <Equivalent context="Common">Pseudo</Equivalent>
                <Example context="Common">333</Example>
            </Attribute>
        </ID>
        <Summary>
            <Attribute name="company" type="String_Long">
                <Documentation>
                    <Description>The IATA assigned code for the Agency or Airine</Description>
                </Documentation>
                <Equivalent context="Common">Company</Equivalent>
                <Example context="Common">WWA</Example>
            </Attribute>
        </Summary>
        <Detail>
            <Attribute name="duty" type="String_Short">
                <Documentation>
                    <Description>An authority code assigned to a requestor.</Description>
                </Documentation>
                <Equivalent context="Common">DutyCode</Equivalent>
                <Example context="Common">ddd</Example>
            </Attribute>
            <Attribute name="lniata" type="Lniata">
                <Documentation>
                    <Description>LNIATA of the Console the agent signed into.</Description>
                </Documentation>
                <Equivalent context="Common">Lniata</Equivalent>
                <Example context="Common">156354</Example>
            </Attribute>
            <Attribute name="sine" type="AgentSine">
                <Documentation>
                    <Description>Identifies the agent or party within the requesting entity.</Description>
                </Documentation>
                <Equivalent context="Common">Sine</Equivalent>
                <Example context="Common">555</Example>
            </Attribute>
            <Attribute name="multiHost" type="String_Short">
                <Documentation>
                    <Description>Airline partition for multi-host source locations.</Description>
                </Documentation>
                <Equivalent context="Common">MultiHost</Equivalent>
                <Example context="Common">WW</Example>
            </Attribute>
            <Element name="Actual" repeat="0" type="POS_Location">
                <Documentation>
                    <Description>Actual is where the transaction is initiated.</Description>
                </Documentation>
                <Equivalent context="Common">Actual</Equivalent>
            </Element>
            <Element name="Home" repeat="100000" type="POS_Location">
                <Documentation>
                    <Description>Home is assigned to the agent account.</Description>
                </Documentation>
                <Equivalent context="Common">Home</Equivalent>
            </Element>
        </Detail>
    </BusinessObject>
    
    <BusinessObject name="Phones" notExtendable="false">
        <Aliases/>
        <ID>
            <Element name="PhoneNumber" repeat="0" type="xsd:string"/>
        </ID>
        <Summary>
            <Element name="HomePhone" repeat="0" type="HomePhone"/>
            <Element name="PhoneDetail" repeat="0" type="Phone_Detail_List"/>
            <Element name="Phone" repeat="0" type="Phone"/>
        </Summary>
        <Detail/>
    </BusinessObject>
    
    <BusinessObject name="PaymentCard" notExtendable="false">
        <Aliases>Card CreditCard</Aliases>
        <ID>
            <Attribute name="property1" type="xsd:ID"/>
        </ID>
        <Summary>
            <Attribute name="expire" type="xsd:date">
                <Example context="Common">0714</Example>
            </Attribute>
            <Attribute name="holder" type="xsd:string">
                <Example context="Common">Joe E Smith</Example>
            </Attribute>
            <Element name="PaymentCard_Simple" repeat="0" type="xsd:string"/>
        </Summary>
        <Detail>
            <Attribute name="type" type="xsd:string">
                <Example context="Common">Visa</Example>
            </Attribute>
            <Attribute name="effective" type="ota2:UTCDate"/>
            <Element name="Issuer" repeat="0" type="xsd:string">
                <Example context="Common">First Bank of Travel</Example>
            </Element>
            <Element name="MagneticStrip" repeat="0" type="xsd:string"/>
            <Indicator name="signatureOnFileInd" publishAsElement="false"/>
            <Indicator name="validInd" publishAsElement="false"/>
        </Detail>
    </BusinessObject>
    
    <ExtensionPointFacet>
        <Attribute name="Employment" type="Employment"/>
        <Attribute name="flight" type="xsd:string"/>
        <Element name="Location" repeat="0" type="Location">
            <Documentation>
                <Description>On location VWA</Description>
            </Documentation>
        </Element>
        <Element name="Phone" repeat="0" type="Phone"/>
        <Element name="Profile" repeat="0" type="Profile"/>
        <Element name="Tax" repeat="0" type="Tax"/>
        <Element name="Payment" repeat="0" type="Payment">
            <Documentation>
                <Description>Amount of the payment.</Description>
            </Documentation>
        </Element>
        <Element name="Amount" repeat="0" type="Amount"/>
        <Element name="PaymentCard" repeat="0" type="PaymentCard"/>
        <Indicator name="Date_MMYYInd" publishAsElement="true">
            <Documentation>
                <Description>Added this description and the a-z regex</Description>
            </Documentation>
        </Indicator>
        <Extension extends="t4:DetailCoreProperty_Detail"/>
    </ExtensionPointFacet>
    
    <CoreObject name="Flight" notExtendable="true">
        <Aliases/>
        <Simple type="ota2:Empty"/>
        <Summary>
            <Element mandatory="true" name="FlightNumber" repeat="0" type="Flight_Number">
                <Documentation>
                    <Description>The flight number associated to the service.   Mapped to: SR05_42_FLTN</Description>
                </Documentation>
                <Equivalent context="Common">FlightNumber</Equivalent>
            </Element>
            <Element mandatory="true" name="DepartureDate" repeat="0" type="xsd:date">
                <Documentation>
                    <Description>The departure date of the flight associated to the service.  Mapped to:
                        SR05_42_CDTE</Description>
                </Documentation>
                <Equivalent context="Common">DepartureDate</Equivalent>
            </Element>
            <Element mandatory="true" name="Carrier" repeat="0" type="Code_Airline_IATA">
                <Documentation>
                    <Description>The carrier code of the flight associated to the service.  Mapped to: 
                        SR05_42_CXRC</Description>
                </Documentation>
                <Equivalent context="Common">CarrierCode</Equivalent>
                <Example context="Common">WW</Example>
            </Element>
            <Element mandatory="true" name="ClassOfService" repeat="0" type="String_Short">
                <Documentation>
                    <Description>The flight class of service associated to the ancillary service.  Mapped to:
                        SR05_42_CLSV</Description>
                </Documentation>
                <Equivalent context="Common">ClassOfService</Equivalent>
                <Example context="Common">F00</Example>
            </Element>
            <Element mandatory="true" name="DepartureCity" repeat="0" type="tga:Code_City">
                <Documentation>
                    <Description>The origin or depature city code of the flight associated to the service.   Mapped to:
                        SR05_42_DCTY</Description>
                </Documentation>
                <Equivalent context="Common">DepartureCity</Equivalent>
                <Example context="Common">LGA</Example>
            </Element>
            <Element mandatory="true" name="ArrivalCity" repeat="0" type="tga:Code_City">
                <Documentation>
                    <Description>The destination or arrival city of the flight associated to the service.   Mapped to:
                        SR05_42_ACTY</Description>
                </Documentation>
                <Equivalent context="Common">ArrivalCity</Equivalent>
                <Example context="Common">MIA</Example>
            </Element>
        </Summary>
        <Detail>
            <Indicator name="stopOverInd" publishAsElement="false">
                <Documentation>
                    <Description>A code that applies to whether the flight is a stop over or not (X, O).   Mapped to:
                        SR05_42_STOV</Description>
                </Documentation>
            </Indicator>
        </Detail>
    </CoreObject>
    
    <ValueWithAttrs name="Currency_Code" type="Currency_Code_ISO4217">
        <Documentation>
            <Description>An ISO 4217 (3) alpha character code that specifies a monetary unit.</Description>
            <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}AlphaLength3</Implementer>
        </Documentation>
        <Equivalent context="Common">CurrencyCodeGroup</Equivalent>
        <Example context="Common">USD</Example>
        <Attribute name="DecimalPlaces" type="xsd:string">
            <Documentation>
                <Description>The ISO 4217 standard "minor unit" is typically used when the amount provided includes the minor unit of currency without a decimal point (e.g., USD 8500 needs DecimalPlaces="2" to represent $85).</Description>
                <Implementer>Changed type from {http://www.w3.org/2001/XMLSchema}nonNegativeInteger.</Implementer>
                <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
            </Documentation>
            <Equivalent context="Common">DecimalPlaces</Equivalent>
            <Example context="Common">2</Example>
        </Attribute>
    </ValueWithAttrs>
    
    <Simple name="Currency_Code_ISO4217" pattern="[a-zA-Z]{3}" type="xsd:string">
        <Documentation>
            <Description>An ISO 4217 (3) alpha character code that specifies a monetary unit.</Description>
            <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
        </Documentation>
    </Simple>
    
    <BusinessObject name="Company" notExtendable="false">
        <Aliases>Employer</Aliases>
        <ID>
            <Attribute name="id" type="xsd:ID"/>
            <Element name="Identifier" repeat="0" type="Identifier">
                <Documentation>
                    <Description>Identifier provides the ability to create a globally unique identifier. For the identifier to be globally unique it must have a system provided identifier and the system must be identified using a global naming authority. System identification uses the domain naming system (DNS) to assure they are globally unique and should be an URL. The system provided ID will typically be a primary or surrogate key in a database.

The URL, system and company attributes can be omitted only when the system context can be implied by a parent or ancestor element.</Description>
                </Documentation>
            </Element>
        </ID>
        <Summary>
            <Element mandatory="true" name="Name" repeat="20" type="CompanyName">
                <Documentation>
                    <Description>Detailed name information for the company.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}CompanyNameType</Implementer>
                </Documentation>
                <Equivalent context="Common">CompanyName</Equivalent>
            </Element>
            <Element name="Address" repeat="100" type="Address">
                <Documentation>
                    <Description>A jurisdiction in which a company is authorized to do business. Uses the Address entity to indicate the location of business using any of the components of the address (e.g., city, state, zip code, country).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}AddressType</Implementer>
                </Documentation>
                <Equivalent context="Common">BusinessLocale</Equivalent>
            </Element>
            <Element name="Phone" repeat="100" type="Phone">
                <Documentation>
                    <Description>Information about a telephone number, including the actual number and its usage.</Description>
                </Documentation>
            </Element>
            <Element name="Email" repeat="100" type="Email">
                <Documentation>
                    <Description>Electronic email addresses, in IETF specified format.</Description>
                </Documentation>
            </Element>
            <Element name="URL" repeat="100" type="URL">
                <Documentation>
                    <Description>Information on a URL for the company.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}URL_Type</Implementer>
                </Documentation>
                <Equivalent context="Common">URL</Equivalent>
            </Element>
        </Summary>
        <Detail/>
    </BusinessObject>
    
    <Simple name="RuralRouteNumber" pattern="[0-9]{1,5}" type="xsd:string">
        <Documentation>
            <Description/>
            <Implementer>Example value: Imported5 = 2564</Implementer>
            <Implementer>Example value: Imported4 = 2564</Implementer>
            <Implementer>Example value: Imported2 = 2564</Implementer>
            <Implementer>Example value: Imported67 = 2564</Implementer>
            <Implementer>Example value: Imported123 = 2564</Implementer>
            <Implementer>Example value: Imported34 = 2564</Implementer>
            <Implementer>Example value: ca1 = 25645</Implementer>
        </Documentation>
    </Simple>
    
    <Simple maxLength="8" minLength="2" name="StateProv_Code" type="xsd:string">
        <Documentation>
            <Description>The standard code or abbreviation for the state, province, or region.</Description>
            <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to8</Implementer>
        </Documentation>
    </Simple>
    
    <Simple name="Telephone_Number" pattern="[0-9]{7,20}" type="xsd:string">
        <Documentation>
            <Description>Central office and subscriber number.</Description>
            <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
        </Documentation>
    </Simple>
    
    <Simple name="Telephone_AreaCityCode" pattern="[0-9]{1,8}" type="xsd:string">
        <Documentation>
            <Description>Code assigned for telephones in a specific region, city, or area.</Description>
            <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
        </Documentation>
    </Simple>
    
    <ValueWithAttrs name="StateProv" type="StateProv_Code">
        <Documentation>
            <Description>The standard code or abbreviation for the state, province, or region.</Description>
            <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StateProvCodeType</Implementer>
        </Documentation>
        <Equivalent context="Common">StateProvType</Equivalent>
        <Example context="Common">TX</Example>
        <Attribute name="name" type="xsd:string">
            <Documentation>
                <Description>State, province, or region name or code needed to identify location.</Description>
                <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength0to64</Implementer>
            </Documentation>
            <Example context="Common">Texas</Example>
        </Attribute>
    </ValueWithAttrs>
    
    <ValueWithAttrs name="Email" type="xsd:string">
        <Documentation>
            <Description>Electronic email addresses, in IETF specified format.</Description>
            <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</Implementer>
        </Documentation>
        <Equivalent context="Common">EmailType</Equivalent>
        <Attribute name="emailType" type="Code_OTA">
            <Documentation>
                <Description>Defines the purpose of the e-mail address (e.g. personal, business, listserve). Refer to OpenTravel Code List Email Address Type (EAT).</Description>
                <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</Implementer>
            </Documentation>
            <Equivalent context="Common">EmailType</Equivalent>
        </Attribute>
        <Attribute name="remark" type="String_Text">
            <Documentation>
                <Description>A remark associated with the e-mail address.</Description>
                <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</Implementer>
            </Documentation>
            <Equivalent context="Common">Remark</Equivalent>
        </Attribute>
        <Attribute name="textFormat" type="xsd:string">
