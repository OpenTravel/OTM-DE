<?xml version="1.0" encoding="UTF-8"?>
<Library xmlns="http://www.OpenTravel.org/ns/OTA2/LibraryModel_v01_05"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.OpenTravel.org/ns/OTA2/LibraryModel_v01_05 OTA2_LibraryModel_v1.5.0.xsd">
    
    <VersionScheme>OTA2</VersionScheme>
    <Status>Draft</Status>
    <Namespace>http://www.opentravel.org/Sandbox/junits/v1</Namespace>
    <Prefix>tga</Prefix>
    <Name>Test3</Name>
    <Includes>testFile5.otm</Includes>
    <Import namespace="http://www.opentravel.org/OTM/Common/v0" prefix="ota2"/>
    <Context applicationContext="Default" context="default"/>
    
    <ValueWithAttrs name="AttributeGroupVWA" type="ota2:Empty">
        <Attribute name="agentDuty" type="String_Short">
            <Documentation>
                <Description>Typically a two to five character code (alpha, numeric, or combination) that specifies a travel agent duty code that is used for agency authentication.</Description>
            </Documentation>
        </Attribute>
        <Attribute name="city" type="String_Short">
            <Documentation>
                <Description>A three character code for a city.</Description>
            </Documentation>
        </Attribute>
        <Attribute name="eRSP" type="String_Short">
            <Documentation>
                <Description>A unique IATA (in cooperation with ATA and ARC) code used as a means of identification when performing online bookings with airlines.</Description>
            </Documentation>
        </Attribute>
        <Attribute name="state" type="String_Short">
            <Documentation>
                <Description>A 2 character abbreviation for a state.</Description>
            </Documentation>
        </Attribute>
        <Attribute name="tour" type="String_Short">
            <Documentation>
                <Description>Up to a 32 character numeric or alphanumeric tour code.</Description>
            </Documentation>
        </Attribute>
    </ValueWithAttrs>
    
    <ValueWithAttrs name="SimpleVWA" type="ota2:Empty">
        <Documentation>
            <Description>full name</Description>
        </Documentation>
        <Attribute name="fullname" type="String_Short">
            <Documentation>
                <Description>A name used to describe a person, place or thing, such as a hotel, airport or company.</Description>
            </Documentation>
        </Attribute>
        <Attribute name="nickname" type="String_Short">
            <Documentation>
                <Description>commonly used name</Description>
            </Documentation>
        </Attribute>
    </ValueWithAttrs>
    
    <ValueWithAttrs name="VWA_as_valueType" type="SimpleVWA">
        <Attribute name="family" type="String_Short">
            <Documentation>
                <Description>A string with a minimum length of 1 and a maximum length of 128.</Description>
            </Documentation>
        </Attribute>
        <Attribute name="aka" type="String_Short">
            <Documentation>
                <Description>A string with a minimum length of 1 and a maximum length of 128.</Description>
            </Documentation>
        </Attribute>
    </ValueWithAttrs>
    
</Library>
