		</xs:attribute>
		<xs:attribute name="FNL" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation>Funnel flight</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EQP" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Equipment code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EQ1" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Equipment for the first leg</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EQT" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Equipment type for the first leg (widebody etc.)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DQ1" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Equipment for the last leg</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DQT" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Equipment type for the last leg (widebody etc.)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LOF" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Line of flight (space-delimited)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ORC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Origin country</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DSC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Destination country</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ORS" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Origin state</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DS2" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Destination state</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TMS" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>segment timestamp in format: YYYY-MM-DD HH:mm:ss</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BRD" type="AirportCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Boarding city for origin airport</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OFF" type="AirportCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Destination city for destination airport</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SGA" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation>Subject to Government Approval (DEI201)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RAR" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Request All Reservation (DEI507)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PAC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Passenger Checkin information (DEI299)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SAC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Set Aside Classes (DEI905)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="SEGType">
		<xs:annotation>
			<xs:documentation>Schedule segment index + availability data</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VAI" type="AncillaryInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about ancillaries (seats, bags, etc.) for this segment
          </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="SID" type="xs:long" use="required">
			<xs:annotation>
				<xs:documentation>Segment index</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CID" type="xs:integer" default="0">
			<xs:annotation>
				<xs:documentation>If present, contains the branding internal campaign ID this segment belongs to</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DDA" type="xs:integer" use="optional" default="0">
			<xs:annotation>
				<xs:documentation>Departure day adjustment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CXC" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Class codes with class statuses if AVL call was requested</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AVC" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>If AVL call was requested, this attribute will contain a list of AVS status codes (2 characters for each availability class)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AVT" type="xs:short" default="0">
			<xs:annotation>
				<xs:documentation>Availability Type - when used for Seamless Codeshare has value of 1</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FST" type="xs:string">
			<xs:annotation>
				<xs:documentation>Flight statused</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MTH" type="xs:integer" default="0">
			<xs:annotation>
				<xs:documentation>Availability method used by ATAE</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RCD" type="xs:integer" default="0">
			<xs:annotation>
				<xs:documentation>Return code from ATAE</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PLI" type="xs:integer" default="0">
			<xs:annotation>
				<xs:documentation>PollingIndicator - returned by ATAE</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="RegionType">
		<xs:annotation>
			<xs:documentation>2-digit region code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d\d"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
