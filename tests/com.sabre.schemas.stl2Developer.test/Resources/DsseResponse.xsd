<xs:schema targetNamespace="http://www.sabre.com/ns/dss/v01" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sabre.com/ns/dss/v01" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:annotation>
		<xs:documentation>dmh - 12/5/2011 - added namespace</xs:documentation>
		<xs:documentation>DSSE Response XML Schema</xs:documentation>
		<xs:documentation>Created: 2005-03-03</xs:documentation>
		<xs:documentation>Author: Prakash Kadamba, Pawel Wyrzycki, Michal Grzemowski, Pawel Bulkowski,
      Bartlomiej Nagorski, Marcin Olszewski</xs:documentation>
		<xs:documentation>Copyright Sabre 2004-2009</xs:documentation>
		<xs:documentation>The copyright to the computer program(s) herein is the property of Sabre. The
      program(s) may be used and/or copied only with of Sabre or in accordance with the terms and
      conditions stipulated in the agreement/contract under which the program(s) have been supplied.
    </xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>Changed 2009-10-01 Marcin Olszewski</xs:documentation>
		<xs:documentation>Added elements related to ancillary information</xs:documentation>
		<xs:documentation>Formatted and removed redundant XML schema definition attributes
    </xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="CommonTypes.xsd"/>
	<xs:element name="DSS" type="DsseResponseType">
		<xs:annotation>
			<xs:documentation>DSSE Response Element. This is a root element of this schema. It consists of
        a number of response items.Each response item corresponds to the appropriate request item of
        DSS Request. In case of completely invalid request, the response may have no items.
      </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="DsseResponseType">
		<xs:annotation>
			<xs:documentation>DSSE Response consists 1-n Response Items. In case of completely invalid
        request, the response may have no RespItems.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>This variant is being used if an error occurred during processing
          </xs:documentation>
				</xs:annotation>
				<xs:element name="ERR" type="DsseErrorType">
					<xs:annotation>
						<xs:documentation>Contains processing error details</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>This variant is used for building a response for a connect point request
          </xs:documentation>
				</xs:annotation>
				<xs:element name="CPL" type="CPLType">
					<xs:annotation>
						<xs:documentation>A response for a connect paths request</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>This variant is used for regular schedule search response, flight
            details response, MCT check response, activity tracking status response or flight counts
            response.</xs:documentation>
				</xs:annotation>
				<xs:choice>
					<xs:sequence>
						<xs:element name="CPG" type="CPGType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>
                        A list og campaigns returned by
                        brading service
                  </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:choice>
							<xs:sequence>
								<xs:annotation>
									<xs:documentation>
                      Regular schedule search response with no display groups (ASL); branding header (if branding requested) - CPG.
                    </xs:documentation>
								</xs:annotation>
								<xs:element name="ASL" type="ASLType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>
                        A list of solutions - response for standard request without grouping
                      </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
							<xs:sequence>
								<xs:element name="CSG" type="CSGType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation> A compact list of schedule segments </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="CSL" type="CSLType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>
                        A compact list of solutions - response for standard request without grouping
                      </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:choice>
						<xs:sequence>
							<xs:annotation>
								<xs:documentation>Solutions list - regular schedule search response with groups</xs:documentation>
							</xs:annotation>
							<xs:element name="DGR" type="DGRType" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>
                      A list of display groups - response for standard request with grouping
                    </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DCK" type="DCKType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>
                      Holds a cookie information - only present for EA DIV display groups
                    </xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:sequence>
					<xs:sequence>
						<xs:annotation>
							<xs:documentation>Flight counts response</xs:documentation>
						</xs:annotation>
						<xs:element name="FSD" type="FSDType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>
        							A response for flight count request
        						</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:sequence>
						<xs:annotation>
							<xs:documentation>
                    Flight details response
                  </xs:documentation>
						</xs:annotation>
						<xs:element name="FLL" type="FLLType">
							<xs:annotation>
								<xs:documentation>
        							A response for a simple request
        						</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:sequence>
						<xs:annotation>
							<xs:documentation>MCT check response</xs:documentation>
						</xs:annotation>
						<xs:element name="MCT" type="MCTType">
							<xs:annotation>
								<xs:documentation>
        							A response for a MCT request
        						</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:sequence>
						<xs:annotation>
							<xs:documentation>MCT check error response</xs:documentation>
						</xs:annotation>
						<xs:element name="MER" type="MERType">
							<xs:annotation>
								<xs:documentation>
        							Contains an error detected during
        							processing of either Simple Request,
                                                                MCT Request or Connection Matrix Request
        						</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:sequence>
						<xs:annotation>
							<xs:documentation>Activity tracking status response</xs:documentation>
						</xs:annotation>
						<xs:element name="ACS" type="ACSType">
							<xs:annotation>
								<xs:documentation>
        							A response for a Activity Status
        							Tracking request
        						</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:choice>
				<xs:element name="ODM" type="ODMsgType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
        					Global message for this city pair
        				</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DIA" type="DIAType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
        					Contains requested diagnostics generated
        					during processing
        				</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PTM" type="PTMType">
					<xs:annotation>
						<xs:documentation>
        					Contains performance timings
        				</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>This variant is being used for a response for connection matrix request</xs:documentation>
				</xs:annotation>
				<xs:element name="CMX" type="CMXRespType">
					<xs:annotation>
						<xs:documentation>
        					A response for a connection matrix request
        				</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>This variant is being used if a request contained ODI CCV attribute with a value of true</xs:documentation>
				</xs:annotation>
				<xs:element name="OCL" type="CarrierCoverageType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Carrier coverage data for origin location(s) - outgoing flights</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DCL" type="CarrierCoverageType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Carrier coverage data for destination location(s) - incoming flights</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="VER" type="xs:string">
			<xs:annotation>
				<xs:documentation>The version of the response</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="COR" type="CorporateIDType">
			<xs:annotation>
				<xs:documentation>Corporate ID (taken from the request)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TXN" type="ClientTransactionIDType">
			<xs:annotation>
				<xs:documentation>Transaction ID (taken from the request)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="DsseErrorType">
		<xs:annotation>
			<xs:documentation>DSSE Response error details</xs:documentation>
		</xs:annotation>
		<xs:attribute name="COD" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation>Error code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MSG" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Error message</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ASLType" mixed="true">
		<xs:annotation>
			<xs:documentation>Details of a single solution</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ASG" type="ASGType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>List of segments the solution consists of</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="SBD" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>If true, indicates this option is to be branded</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TTT" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Total travel time</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TLT" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Total layover time</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ASGType">
		<xs:annotation>
			<xs:documentation>Details of a single segment</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="HSG" type="HSGType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A list of hidden stops</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ALG" type="ALGType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Detailed leg information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SOM" type="ODMsgType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Solution message for this city pair
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VAI" type="AncillaryInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about ancillaries (seats, bags, etc.) for this segment
          </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="CID" type="xs:integer">
			<xs:annotation>
				<xs:documentation>If present, contains the branding internal campaign ID this segment belongs to</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DDA" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>departure day adjustment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BBR" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>is the carrier a BBR carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="NPR" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>is the carrier a non-participant carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ICD" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>is the carrier a call direct carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CTT" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>is the carrier a check tariff carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SSA" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>is the carrier an availability suppress carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BCC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Currently unused</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ORG" type="AirportCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Origin airport</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DST" type="AirportCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Destination airport</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MXC" type="AirlineCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Marketing carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FLT" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Marketing flight number</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MFS" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Marketing flight suffix</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ED1" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation>Effective date </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ED2" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation>Discontinue date</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ED3" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Frequency</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ED4" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Aggregated frequency</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SCH" type="xs:long" use="required">
			<xs:annotation>
				<xs:documentation>DB key of this schedule segment (schedule ID)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ODA" type="xs:short" use="required">
			<xs:annotation>
				<xs:documentation>Departure day adjust relative to LOF (line of flight) origin</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ODD" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Local departure time HH::MM</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OGA" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>GMT adjustment for departure time</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OLN" type="xs:unsignedShort" use="required">
			<xs:annotation>
				<xs:documentation>The leg number within LOF</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OTC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Departure terminal ID</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DD2" type="xs:short" use="required">
			<xs:annotation>
				<xs:documentation>Arrival day adjust relative to boardpoint</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DSA" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Local arrive time</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DGA" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>GMT adjustment for arrival time</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="STC" type="xs:unsignedShort" use="required">
			<xs:annotation>
				<xs:documentation>Count of stops in LOF</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DTC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Arrival terminal ID</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IDA" type="xs:short" use="required">
			<xs:annotation>
				<xs:documentation>Index for date adjust - for PSS</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CXC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Class codes, optionally with class statuses if AVL call was requested</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AVC" type="xs:string">
			<xs:annotation>
				<xs:documentation>If AVL call was requested, this attribute will contain a list of AVS status codes (2 characters for each availability class)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AVT" type="xs:short" default="0">
			<xs:annotation>
				<xs:documentation>Availability Type - when used for Seamless Codeshare has value of 1</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MX2" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Meal service</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OCX" type="AirlineCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Operating carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OFN" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Operating flight number</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OPC" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Operating carrier pricing</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DCX" type="xs:string">
			<xs:annotation>
				<xs:documentation>Disclosure carrier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MXX" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Other carriers and flight numbers marketing a base flight - space-delimited</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ALX" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Alliances - eg. One World - OW</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TRS" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Flight tracking status (Dom. vs Intl)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TRX" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Traffic restriction</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TRA" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Traffic restriction application</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ONT" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>On-time performance indicator</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DOT" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>DOT disclosure</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="S07" type="xs:string">
			<xs:annotation>
				<xs:documentation>DOT disclosure message</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LGS" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Code Share exists</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ETX" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>E-ticket indicator</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SMK" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Smoking allowed</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CHT" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Charter flight</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FNL" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Funnel flight</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EQP" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Equipment code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EQ1" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Equipment for the first leg</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EQT" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Equipment type for the first leg (widebody etc.)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DQ1" type="xs:string" use="required">
