  <xsd:complexType name="Flight.Identifier">
    <xsd:annotation>
      <xsd:documentation>Flight identifier is a flight number. It identifies a specific flight or a group of flights with the same number.  The optional carrier and date attributes can be used to override the default assumed values.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="FlightNumber">
        <xsd:attribute name="carrier" type="AirlineCode" use="required">
          <xsd:annotation>
            <xsd:documentation>By default, the carrier is assumed from the transaction context</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="date" use="required">
          <xsd:annotation>
            <xsd:documentation>By default, the date is assumed to be today or unspecified identifing a group of flights with the same number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Flight.Identifier.SSI">
    <xsd:annotation>
      <xsd:documentation>Flight identifier is a flight number. It identifies a specific flight or a group of flights with the same number.  The optional carrier and date attributes can be used to override the default assumed values.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="FlightNumber.Base">
        <xsd:attribute name="carrier" type="AirlineCode" use="required">
          <xsd:annotation>
            <xsd:documentation>By default, the carrier is assumed from the transaction context</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="date" type="Date" use="required">
          <xsd:annotation>
            <xsd:documentation>By default, the date is assumed to be today or unspecified identifing a group of flights with the same number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="HotelName">
    <xsd:annotation>
      <xsd:documentation>Contents are the hotel name, a 255 character string.
"ChainCode" represents the hotel chain code.
"HotelCode" represents the hotel property code.
"HotelCityCode" represents the hotel location code.
Example: <HotelName ChainCode="LQ" CityCode="DFW" HotelCode="9508">LA QUINTA DALLAS LEWISVILLE</HotelName>
SabreWebServices = BasicPropertyInfo
			</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="StringLength1to255">
        <xsd:attribute name="ChainCode" type="StringLength0to32"/>
        <xsd:attribute name="HotelCode" type="StringLength0to32"/>
