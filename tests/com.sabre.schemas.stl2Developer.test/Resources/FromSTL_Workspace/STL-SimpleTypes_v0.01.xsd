  <xsd:complexType name="Telephone">
    <xsd:annotation>
      <xsd:documentation>Unstructured telephone number and attributes. Optional attributes describe usage, type and structured phone number fields.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="PhoneNumber">
        <xsd:attribute name="location">
          <xsd:annotation>
            <xsd:documentation>List will be based on PPP Codes
						Refer to OTA Code List Phone Location Type (PLT).&#13;
	1	Brand reservations office&#13;
	2	Central reservations office&#13;
	3	Property reservation Office&#13;
	4	Property direct&#13;
	5	Sales office&#13;
	6	Home&#13;
	7	Office&#13;
	8	Other&#13;
	9	Managing company</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="use" type="StringLength1to5">
          <xsd:annotation>
            <xsd:documentation>List will be based on PPP Codes

						Describes the type of telephone number, in the context of its general use (e.g. Home, Business, Emergency Contact, Travel Arranger, Day, Evening). Refer to OTA Code List Phone Use Type (PUT).&#13;
1	Emergency contact&#13;
2	Travel arranger&#13;
3	Daytime contact&#13;
4	Evening contact&#13;
5	Contact&#13;
6	Toll free number&#13;
7	Guest use&#13;
8	Pickup contact</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="countryCode">
          <xsd:annotation>
            <xsd:documentation>Code assigned by telecommunications authorities for international country access identifier.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="extension">
          <xsd:annotation>
            <xsd:documentation>Extension to reach a specific party at the phone number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="remark">
          <xsd:annotation>
            <xsd:documentation>A remark associated with the telephone number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Telephone.OTA">
    <xsd:annotation>
      <xsd:documentation>Unstructured telephone number and attributes. Optional attributes describe usage, type and structured phone number fields.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="PhoneNumber">
        <xsd:attribute name="location" type="OTA_CodeType">
          <xsd:annotation>
            <xsd:documentation>Refer to OTA Code List Phone Location Type (PLT).&#13;
	1	Brand reservations office&#13;
	2	Central reservations office&#13;
	3	Property reservation Office&#13;
	4	Property direct&#13;
	5	Sales office&#13;
	6	Home&#13;
	7	Office&#13;
	8	Other&#13;
	9	Managing company&#13;
</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="use" type="OTA_CodeType">
          <xsd:annotation>
            <xsd:documentation>Describes the type of telephone number, in the context of its general use (e.g. Home, Business, Emergency Contact, Travel Arranger, Day, Evening). Refer to OTA Code List Phone Use Type (PUT).&#13;
1	Emergency contact&#13;
2	Travel arranger&#13;
3	Daytime contact&#13;
4	Evening contact&#13;
5	Contact&#13;
6	Toll free number&#13;
7	Guest use&#13;
8	Pickup contact&#13;
</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="countryCode" type="NumericStringLength1to3">
          <xsd:annotation>
            <xsd:documentation>Code assigned by telecommunications authorities for international country access identifier.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="areaCode" type="NumericStringLength1to8">
          <xsd:annotation>
            <xsd:documentation>Code assigned for telephones in a specific region, city, or area.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="extension" type="StringLength1to5">
          <xsd:annotation>
            <xsd:documentation>Extension to reach a specific party at the phone number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PIN" type="StringLength1to8">
          <xsd:annotation>
            <xsd:documentation>Additional codes used for pager or telephone access rights.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="remark" type="Text">
          <xsd:annotation>
            <xsd:documentation>A remark associated with the telephone number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="TimeSpan">
    <xsd:simpleContent>
      <xsd:extension base="xsd:duration">
        <xsd:attribute name="Start" type="xsd:dateTime"/>
        <xsd:attribute name="End" type="xsd:dateTime"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="UniqueID">
    <xsd:annotation>
      <xsd:documentation>A unique identifying value assigned by the creating system.  The ID attribute may be used to reference a primary-key value within a database or in a particular implementation.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="StringLength1to32">
        <xsd:attribute name="URL" type="xsd:anyURI">
          <xsd:annotation>
            <xsd:documentation>URL that identifies the location associated with the record identified by the UniqueID.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Type" type="OTA_CodeType">
          <xsd:annotation>
            <xsd:documentation>A reference to the type of object defined by the UniqueID element. Refer to OTA Code List Unique ID Type (UIT).</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CompanyName" type="StringLength1to128">
          <xsd:annotation>
            <xsd:documentation>Identifies the company that is associated with the UniqueID.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ID_Context" type="StringLength1to32">
          <xsd:annotation>
            <xsd:documentation>Used to identify the source of the identifier (e.g., IATA, ABTA).</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Vendor">
    <xsd:annotation>
      <xsd:documentation>Vendor code and additional optional attributes.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="VendorCode">
        <xsd:attribute name="Name" type="ProperName"/>
        <xsd:attribute name="CodeContext">
          <xsd:annotation>
            <xsd:documentation>Identifies the context of the identifying code, such as DUNS, IATA or internal code, etc.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TravelSector" type="TravelSectorType"/>
        <xsd:attribute name="Division" type="ProperName"/>
        <xsd:attribute name="Department" type="ProperName"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="VendorPreference">
    <xsd:annotation>
      <xsd:documentation>Preferred vendor company by code and optional name.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="VendorCode">
        <xsd:attribute name="PreferLevel" type="PreferLevelType">
          <xsd:annotation>
            <xsd:documentation>Used to indicate a level of preference for an associated item.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="ProperName">
          <xsd:annotation>
            <xsd:documentation>Full name of the prefered vendor.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="AddressType.ACD.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="A"/>
      <xsd:enumeration value="C"/>
      <xsd:enumeration value="D"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AddressType.RD.PNRB">
    <xsd:annotation>
      <xsd:documentation>R = Residence D = Destination</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="R"/>
      <xsd:enumeration value="D"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Age.PNRB">
    <xsd:restriction base="xsd:int">
      <xsd:minExclusive value="0"/>
      <xsd:maxExclusive value="99"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AgentSine">
    <xsd:annotation>
      <xsd:documentation>Identifies a specific party within a POS entity.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength1to16"/>
  </xsd:simpleType>
  <xsd:simpleType name="AirSegmentType.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="REG"/>
      <xsd:enumeration value="ARNK"/>
      <xsd:enumeration value="OPEN"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AirTripDirectionType">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Outbound"/>
      <xsd:enumeration value="Return"/>
      <xsd:enumeration value="All"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AirTripType">
    <xsd:annotation>
      <xsd:documentation>Identifies the trip type - one way, return, circle trip, open jaw</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="OneWay"/>
      <xsd:enumeration value="OneWayOnly"/>
      <xsd:enumeration value="Return"/>
      <xsd:enumeration value="Circle"/>
      <xsd:enumeration value="OpenJaw"/>
      <xsd:enumeration value="Other"/>
      <xsd:enumeration value="Outbound"/>
      <xsd:enumeration value="OutboundSeasonRoundtrip"/>
      <xsd:enumeration value="Non-directional"/>
      <xsd:enumeration value="Inbound"/>
      <xsd:enumeration value="Roundtrip"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AircraftConfiguration">
    <xsd:annotation>
      <xsd:documentation>TODO - add constraints</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="AircraftRegistration">
    <xsd:annotation>
      <xsd:documentation>The symbols in the constraining pattern facet allow for dash and underscore which are can be found in registration identifiers.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[\S]{2,10}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AircraftRegistration.SSIM">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9]{2,10}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AircraftType">
    <xsd:annotation>
      <xsd:documentation>Supports both ICAO and IATA standards as well as the commly used subfleet type (3 characters).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9]{3,4}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AircraftType.SSIM">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9]{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AircraftVersion">
    <xsd:annotation>
      <xsd:documentation>TODO - add constraints</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="AirlineCode">
    <xsd:annotation>
      <xsd:documentation>Preferred format for airline codes. Structure is 2 upper case alpha-numeric characters followed by an optional uppercase letter.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9]{2}[A-Z]?"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AirlineCode.2to4Char">
    <xsd:annotation>
      <xsd:documentation>2 to 4 character code for an airline. Note: OTA uses a 3 character field.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength2to4"/>
  </xsd:simpleType>
  <xsd:simpleType name="AirlineCode.IATA">
    <xsd:annotation>
      <xsd:documentation>Type definition for 3 or 2 letter airline codes</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="2"/>
      <xsd:maxLength value="3"/>
      <xsd:pattern value="([A-Z]{3}|[A-Z]{2})|([0-9][A-Z])|([A-Z][0-9])"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AirlineCode.OTA">
    <xsd:annotation>
      <xsd:documentation>OTA uses a 3 character field.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength3"/>
  </xsd:simpleType>
  <xsd:simpleType name="AirlineCodeOrSpecification">
    <xsd:union memberTypes="AirlineCode AirlineSpecification"/>
  </xsd:simpleType>
  <xsd:simpleType name="AirlineCodes">
    <xsd:list itemType="AirlineCode"/>
  </xsd:simpleType>
  <xsd:simpleType name="AirlineSpecification">
    <xsd:annotation>
      <xsd:documentation>TODO - add constraints</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="AirportCode">
    <xsd:annotation>
      <xsd:documentation>Same as IATA airport code format.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="UpperCaseAlphaNumericLength3to5"/>
  </xsd:simpleType>
  <xsd:simpleType name="AirportCode.SSIM">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z]{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaLength1">
    <xsd:annotation>
      <xsd:documentation>Used for an Alpha String, length exactly 1.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]{1}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaLength1to2">
    <xsd:annotation>
      <xsd:documentation>Used for an Alpha String, length 1 to 2. </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]{1,2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaLength1to3">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]{1,3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaLength1to6">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]{1,6}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaLength2">
    <xsd:annotation>
      <xsd:documentation>Used for an Alpha String, length exactly 2.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]{2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaLength2to3">
    <xsd:annotation>
      <xsd:documentation>Used for an Alpha String, length 2 to 3.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]{2,3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaLength3">
    <xsd:annotation>
      <xsd:documentation>Used for an Alpha String, length exactly 3.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaLength4">
    <xsd:annotation>
      <xsd:documentation>Used for an Alpha String, length exactly 4.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z]{4}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaNumericStringLength1">
    <xsd:annotation>
      <xsd:documentation>Used for Alpha-Numeric Strings, length 1.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-zA-Z]{1}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaNumericStringLength1to10">
    <xsd:annotation>
      <xsd:documentation>Used for Alpha-Numeric Strings, length 1 to 8.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-zA-Z]{1,10}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaNumericStringLength1to14">
    <xsd:annotation>
      <xsd:documentation>Used forAlpha-Numeric Strings, length 1 to 14.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-zA-Z]{1,14}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaNumericStringLength1to19">
    <xsd:annotation>
      <xsd:documentation>Used forAlpha-Numeric Strings, length 1 to 19.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-zA-Z]{1,19}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaNumericStringLength1to5">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-zA-Z]{1,5}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaNumericStringLength1to8">
    <xsd:annotation>
      <xsd:documentation>Used for Alpha-Numeric Strings, length 1 to 8.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-zA-Z]{1,8}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaSpaceLength1to30">
    <xsd:annotation>
      <xsd:documentation>Used for an Alpha and/or Spaces String, length 1 to 30.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z ]{1,30}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlphaSpaceLength2to35">
    <xsd:annotation>
