  <xsd:simpleType name="NumericStringLength1to10">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,10}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to13">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,13}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to16">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings, length 1 to 16.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,16}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to19">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings, length 1 to 19.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,19}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to2">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to20">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,20}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to3">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings length 1 to 3.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to4">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,4}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to5">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings, length 1 to 5.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,5}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to62">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings, length 1 to 62.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,62}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to7">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,7}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to8">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings, length 1 to 8.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,8}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength1to9">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings, length 1 to 9.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{1,9}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength2">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings, length exactly 2.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength2to3">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{2,3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength3to4">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{3,4}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NumericStringLength4">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric Strings, length exactly 4.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{4}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OTA_CodeType">
    <xsd:annotation>
      <xsd:documentation>Used for codes in the OTA code tables. Possible values of this pattern are 1, 101, 101.EQP, or 101.EQP.X.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9A-Z]{1,3}(\.[A-Z]{3}(\.X){0,1}){0,1}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OfficeLocationType">
    <xsd:annotation>
      <xsd:documentation>Specifies the location type of an office (e.g. main, field, division).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Main"/>
      <xsd:enumeration value="Field"/>
      <xsd:enumeration value="Division"/>
      <xsd:enumeration value="Regional"/>
      <xsd:enumeration value="Remote"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OnFail.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="UPDATE_SESSION"/>
      <xsd:enumeration value="UPDATE_AND_FILE"/>
      <xsd:enumeration value="REJECT_ALL_UPDATES"/>
      <xsd:enumeration value="UPDATE_SESSION_REJECT_FILE_REQUEST"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OnTimePerformance">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([0-9]|[0-9] |[0-9]{2}|[0-9]{2} |U|U |N|N )[A-Z]{3}[0-9]{2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OperationTime">
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>
  <xsd:simpleType name="OperationType.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="C"/>
      <xsd:enumeration value="U"/>
      <xsd:enumeration value="D"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PMS_ResStatusType">
    <xsd:annotation>
      <xsd:documentation>Statuses that exist in a property management system (PMS).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength1to16">
      <xsd:enumeration value="Reserved"/>
      <xsd:enumeration value="Requested"/>
      <xsd:enumeration value="Request denied"/>
      <xsd:enumeration value="No-show"/>
      <xsd:enumeration value="Cancelled"/>
      <xsd:enumeration value="In-house"/>
      <xsd:enumeration value="Checked out"/>
      <xsd:enumeration value="Waitlisted"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="POSType.PNRB">
    <xsd:annotation>
      <xsd:documentation>TODO - what format is this data? Or, should this be POS_Type as is defined elsewhere? Used in 3 of the 4 Message root RQ/RS elements.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="PRBD">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([A-Z][0-9]*)+"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PassengerCount">
    <xsd:restriction base="xsd:int"/>
  </xsd:simpleType>
  <xsd:simpleType name="PassengerTerminal">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9]{1,2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PassengerType.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="I"/>
      <xsd:enumeration value="S"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PaxMessageType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ACTUAL_PAX"/>
      <xsd:enumeration value="ESTIMATE_PAX"/>
      <xsd:enumeration value="BOOKED_PAX"/>
      <xsd:enumeration value="BOARDED_PAX"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PaxType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="REV"/>
      <xsd:enumeration value="NREV"/>
      <xsd:enumeration value="INF"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PaymentCardCode">
    <xsd:annotation>
      <xsd:documentation>The 2 digit code that identifies the credit card.</xsd:documentation>
    </xsd:annotation>
    <xsd:union memberTypes="_PaymentCardType _PaymentCardType_Other"/>
  </xsd:simpleType>
  <xsd:simpleType name="PaymentCardNumber">
    <xsd:restriction base="NumericStringLength1to19"/>
  </xsd:simpleType>
  <xsd:simpleType name="Percentage">
    <xsd:annotation>
      <xsd:documentation>Used for percentage values.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="0.00"/>
      <xsd:maxInclusive value="100.00"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PersonNameType">
    <xsd:annotation>
      <xsd:documentation>Former&#13;
Nickname&#13;
Alternate&#13;
Maiden</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength0to32">
      <xsd:enumeration value="Former"/>
      <xsd:enumeration value="Nickname"/>
      <xsd:enumeration value="Alternate"/>
      <xsd:enumeration value="Maiden"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PhoneLocationType.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="A"/>
      <xsd:enumeration value="H"/>
      <xsd:enumeration value="B"/>
      <xsd:enumeration value="C"/>
      <xsd:enumeration value="M"/>
      <xsd:enumeration value="F"/>
      <xsd:enumeration value="HTL"/>
      <xsd:enumeration value="UNFORMATTED"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PhoneNumber">
    <xsd:restriction base="StringLength1to16"/>
  </xsd:simpleType>
  <xsd:simpleType name="PointOfSaleID">
    <xsd:annotation>
      <xsd:documentation>Point of Sale ID. The 1 to 8 alpha numeric characters Point of Sale (POS) identifies the party or connection channel making the request.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength1to8"/>
  </xsd:simpleType>
  <xsd:simpleType name="PreferLevelType">
    <xsd:annotation>
      <xsd:documentation>Used to specify a preference level for something that is or will be requested (e.g. a supplier of a service, a type of service, a form of payment, etc.).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Only"/>
      <xsd:enumeration value="Unacceptable"/>
      <xsd:enumeration value="Preferred"/>
      <xsd:enumeration value="Required"/>
      <xsd:enumeration value="NoPreference"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PricingSourceType">
    <xsd:annotation>
      <xsd:documentation>Use to indicate whether the fare is public or private.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Published"/>
      <xsd:enumeration value="Private"/>
      <xsd:enumeration value="Both"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PricingType">
    <xsd:annotation>
      <xsd:documentation>An enumerated type that defines how a service is priced.  Values:  Per stay,  Per person, Per night, Per person per night,  Per use.</xsd:documentation>
      <xsd:documentation>TODO - can this be more generic than this Hotel specific list?</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength1to32">
      <xsd:enumeration value="Per stay"/>
      <xsd:enumeration value="Per person"/>
      <xsd:enumeration value="Per night"/>
      <xsd:enumeration value="Per person per night"/>
      <xsd:enumeration value="Per use"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ProfileType">
    <xsd:annotation>
      <xsd:documentation>Type of the Profile TVL-Traveler, CRP-Corporation, AGT- Agent, AGY-Agency, OPX-Operational Profiles</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="TVL"/>
      <xsd:enumeration value="AGT"/>
      <xsd:enumeration value="AGY"/>
      <xsd:enumeration value="CRP"/>
      <xsd:enumeration value="OPX"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ProperName">
    <xsd:restriction base="StringLength1to100"/>
  </xsd:simpleType>
  <xsd:simpleType name="PseudoCityCode">
    <xsd:annotation>
      <xsd:documentation>An identification code assigned to an office/agency by a reservation system.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength1to16"/>
  </xsd:simpleType>
  <xsd:simpleType name="QueueNumber.PNRB">
    <xsd:restriction base="xsd:short">
      <xsd:minExclusive value="50"/>
      <xsd:maxExclusive value="511"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RateIndicator">
    <xsd:annotation>
      <xsd:documentation>OTA 1.8 - RateIndicatorType - An enumerated type indicating special conditions with the rate  Valid values: ChangeDuringStay, 	MultipleNights, Exclusive, OnRequest, LimitedAvailability.</xsd:documentation>
      <xsd:documentation>TODO - can this be more generic than this Hotel specific list?</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength1to32">
      <xsd:enumeration value="ChangeDuringStay"/>
      <xsd:enumeration value="MultipleNights"/>
      <xsd:enumeration value="Exclusive"/>
      <xsd:enumeration value="OnRequest"/>
      <xsd:enumeration value="LimitedAvailability"/>
      <xsd:enumeration value="AvailableForSale"/>
      <xsd:enumeration value="ClosedOut"/>
      <xsd:enumeration value="OtherAvailable"/>
      <xsd:enumeration value="UnableToProcess"/>
      <xsd:enumeration value="NoAvailability"/>
      <xsd:enumeration value="RoomTypeClosed"/>
      <xsd:enumeration value="RatePlanClosed"/>
      <xsd:enumeration value="LOS_Restricted"/>
      <xsd:enumeration value="Restricted"/>
      <xsd:enumeration value="DoesNotExist"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RatePeriodSimpleType">
    <xsd:annotation>
      <xsd:documentation>The RatePeriodSimpleType simple type defines a set of valid values for the type of rate that may be applied.  Typically rates differ based upon the duration, and the actual rate is then expressed in terms of the period of the rental.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Hourly"/>
      <xsd:enumeration value="Daily"/>
      <xsd:enumeration value="Weekly"/>
      <xsd:enumeration value="Monthly"/>
      <xsd:enumeration value="WeekendDay"/>
      <xsd:enumeration value="Other"/>
      <xsd:enumeration value="Package"/>
      <xsd:enumeration value="Bundle"/>
      <xsd:enumeration value="Total"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ReasonCode">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9]{2,4}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Relationships.Personal">
    <xsd:annotation>
      <xsd:documentation>Set of personal relationships of interest to travel planning.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Spouse"/>
      <xsd:enumeration value="Parent"/>
      <xsd:enumeration value="Family"/>
      <xsd:enumeration value="Employer"/>
      <xsd:enumeration value="TravelCompanion"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RelativeArrivalDay">
    <xsd:restriction base="xsd:integer">
      <xsd:minInclusive value="-1"/>
      <xsd:maxExclusive value="2"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RelativeDepartureDayOffset">
    <xsd:annotation>
      <xsd:documentation>Offset in number of 24-hour periods, of the Flight-leg's local departure&#13;
            date from the Flight's (local) Origination Date. An offset of 0 is the default,&#13;
            meaning FlightOriginationDate = LegDate</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="-1"/>
      <xsd:enumeration value="0"/>
      <xsd:enumeration value="1"/>
      <xsd:enumeration value="2"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RemarkPrintCode.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ITIN"/>
      <xsd:enumeration value="INV"/>
      <xsd:enumeration value="PAY"/>
      <xsd:enumeration value="NAME"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RemarkType.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="HS"/>
      <xsd:enumeration value="HD"/>
      <xsd:enumeration value="REG"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RequestType.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="A"/>
      <xsd:enumeration value="G"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RestrictedPayload">
    <xsd:annotation>
      <xsd:documentation>Restriction of standard payload for a given leg. Expresses restriction in either Lbs or Kgs</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([0-9]{1,6})(K|L)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Save.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ON_SUCCESS"/>
      <xsd:enumeration value="NEVER"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ScheduleName">
    <xsd:annotation>
      <xsd:documentation>TODO - add constraints</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="Score">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="100"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SeatDirection">
    <xsd:annotation>
      <xsd:documentation>Identifies the orientation of a seat relative to the direction of  travel.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Facing"/>
      <xsd:enumeration value="Back"/>
      <xsd:enumeration value="Airline"/>
      <xsd:enumeration value="Lateral"/>
      <xsd:enumeration value="Unknown"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SeatType">
    <xsd:annotation>
      <xsd:documentation>Identifies the position of a seat, e.g. Window, Aisle etc.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Window"/>
      <xsd:enumeration value="Aisle"/>
      <xsd:enumeration value="Table"/>
      <xsd:enumeration value="Middle"/>
      <xsd:enumeration value="Individual"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SequenceNumber">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SerialNumber">
    <xsd:annotation>
      <xsd:documentation>TODO - add constraints</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:positiveInteger"/>
  </xsd:simpleType>
  <xsd:simpleType name="ServiceType">
    <xsd:annotation>
      <xsd:documentation>This single character element is broader than the specific list from IATA. Airlines sometimes use a differnt codeset or extend the IATA code set.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z]"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ServiceType.IATA">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([J|S|U|F|V|M|Q|G|B|A|R|C|O|H|L|P|T|K|D|E|W|X])"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ShareIndicator">
    <xsd:annotation>
      <xsd:documentation>Permission for sharing data for information held by other travel service providers. Indicator can be used to indicate independent permission for synchronization or for marketing purposes.  Inherit means to adopt permission from the containing record.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Yes"/>
      <xsd:enumeration value="No"/>
      <xsd:enumeration value="Inherit"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ShortDescriptionType">
    <xsd:annotation>
      <xsd:documentation>A textual description.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength0to64"/>
  </xsd:simpleType>
  <xsd:simpleType name="SmokingCode.PNRB">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NS"/>
      <xsd:enumeration value="SM"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SpecialLegStatus">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Diverted"/>
      <xsd:enumeration value="AirReturn"/>
      <xsd:enumeration value="GroundReturn"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SpecialServiceType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="WCHC"/>
      <xsd:enumeration value="ELECTRIC WCHC"/>
      <xsd:enumeration value="WCHC OWN"/>
      <xsd:enumeration value="UMNR"/>
      <xsd:enumeration value="STCR"/>
      <xsd:enumeration value="BLND"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StateProvCode">
    <xsd:annotation>
      <xsd:documentation>The standard code or abbreviation for the state, province, or region.</xsd:documentation>
      <xsd:documentation>TODO - should this be from a code list?</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength1to8">
      <xsd:minLength value="2"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringLength0to1023">
    <xsd:annotation>
      <xsd:documentation>Used for Character Strings, length 0 to 255.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="1023"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringLength0to128">
    <xsd:annotation>
      <xsd:documentation>Used for Character Strings, length 0 to 128.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="128"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringLength0to16">
    <xsd:annotation>
      <xsd:documentation>Used for Character Strings, length 0 to 8.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="16"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringLength0to255">
    <xsd:annotation>
      <xsd:documentation>Used for Character Strings, length 0 to 255.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringLength0to32">
    <xsd:annotation>
      <xsd:documentation>Used for Character Strings, length 0 to 32.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="32"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringLength0to4096">
    <xsd:annotation>
