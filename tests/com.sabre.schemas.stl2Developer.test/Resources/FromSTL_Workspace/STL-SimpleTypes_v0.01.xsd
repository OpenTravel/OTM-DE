  <xsd:simpleType name="UserId.SSIM">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Za-z][A-Za-z0-9]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="UserType">
    <xsd:annotation>
      <xsd:documentation>TODO - this should be an enumerated list of values</xsd:documentation>
      <xsd:documentation>Requesting user type. For Ex: A for Airline agent, Y for yield management.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="1"/>
      <xsd:pattern value="[A-Z]"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="VendorCode">
    <xsd:annotation>
      <xsd:documentation>4/22/2009 - expanded to 10 characters to accomodate non-air vendors. Vendor Code - upto 6 characters. Use only for attributes, use Vendor for element type declarations.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="StringLength1to10"/>
  </xsd:simpleType>
