  </xsd:complexType>
  <xsd:complexType name="OriginAndDestinationCriteria">
    <xsd:annotation>
      <xsd:documentation>Origin And Destination Criteria:
				Segment Grouping
				Departure Airport Code
				Arrival Airport Code</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Origin" type="STL:AirportCode.Merchandising"/>
      <xsd:element name="Destination" type="STL:AirportCode.Merchandising"/>
      <xsd:element maxOccurs="unbounded" name="SegmentGrouping" type="SegmentGrouping"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OriginAndDestinationResult">
    <xsd:annotation>
      <xsd:documentation>OriginAndDestinationResult
				Origin
				Destination
				AncillaryData</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Origin" type="STL:AirportCode.Merchandising"/>
      <xsd:element name="Destination" type="STL:AirportCode.Merchandising"/>
      <xsd:element name="AncillaryData" type="AncillaryData"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PassengerDetails">
    <xsd:annotation>
      <xsd:documentation>Passenger Details:&#13;
	Customer Value Score (1 to 100)&#13;
        Ticketing Date&#13;
        Checkin Date</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CustomerValueScore" type="STL:CV_Score"/>
      <xsd:element minOccurs="0" name="TicketingDate" type="STL:DateYYYY-MM-DD.Merchandising"/>
      <xsd:element minOccurs="0" name="CheckInDate" type="STL:DateYYYY-MM-DD.Merchandising"/>
      <xsd:element minOccurs="0" name="FrequentFlyerTier" type="STL:StringLength1to64"/>
      <xsd:element minOccurs="0" name="NumberOfBags" type="STL:BagCount"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SegmentAttributes">
    <xsd:annotation>
      <xsd:documentation>Segment Attributes:
				ClassesOfService - RBD/Booking Code/Classes of Service (separated by "|", e.g., F|C|Y)
				StatusValues - Number of Seats available for the classes of service specified in the ClassesOfService list</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="FlightNumberRange" type="STL:StringLength1to255"/>
      <xsd:element minOccurs="0" name="EquipmentType" type="STL:UpperCaseAlphaNumericLength1to3"/>
      <xsd:element minOccurs="0" name="Cabin" type="STL:UpperCaseAlphaLength1"/>
      <xsd:element minOccurs="0" name="ClassesOfService" type="STL:UpperCaseStringLength0to64"/>
      <xsd:element minOccurs="0" name="UpgradeToClassOfService" type="STL:UpperCaseStringLength0to64"/>
      <xsd:element minOccurs="0" name="StatusValues" type="StatusValuesType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SegmentData.Merchandising">
    <xsd:annotation>
      <xsd:documentation>Segment Data:
				SegmentNumber - Segment Number (1, 2 or 3; i.e., segment 2 of a connection)
				SegmentsInConnection - Number of Segments in a Connection (2 or 3)</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="SegmentNumber" type="STL:Numeric1to3"/>
      <xsd:element name="SegmentsInConnection" type="STL:Numeric1to3"/>
      <xsd:element name="AirlineCode" type="STL:AirlineCode.Merchandising"/>
      <xsd:element minOccurs="0" name="FlightNumber" type="STL:FlightNumber.Base"/>
      <xsd:element name="DepartureDate" type="STL:DateYYYY-MM-DD.Merchandising"/>
      <xsd:element minOccurs="0" name="TicketingDate" type="STL:DateYYYY-MM-DD.Merchandising"/>
      <xsd:element name="Origin" type="STL:AirportCode.Merchandising"/>
      <xsd:element name="Destination" type="STL:AirportCode.Merchandising"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SegmentGrouping">
    <xsd:annotation>
      <xsd:documentation>Segment Grouping:
         A Segment Grouping may consist of a direct flight (one AirlineSegmentCriteria),
                                   a 2-segment connection (two AirlineSegmentCriteria), or
                                   a 3-segment connection (three AirlineSegmentCriteria)</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="AirlineSegmentCriteria" type="AirlineSegmentCriteria"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Source.Merchandising">
    <xsd:annotation>
      <xsd:documentation>Source:
				PseudoCityCode - Travel Agency Pseudo City Code; Point of Sale PCC
				CityCode - Agent City Code; Point of Sale City Code
				Client - (e.g., AVL - Availability, PRS - Pre Reserved Seats, ACS - Airport Check In, PNR - Passenger Name Record,
								 SSW - Sabre Sonic Web, INT - InterAct)
				Transaction ID - Client supplied transaction ID</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="PseudoCityCode" type="STL:PseudoCityCode.Merchandising"/>
      <xsd:element minOccurs="0" name="CityCode" type="STL:AirportCode.Merchandising"/>
      <xsd:element name="ClientID" type="STL:ClientID.Merchandising"/>
      <xsd:element minOccurs="0" name="TransactionID" type="STL:UpperCaseStringLength1to64"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="StatusValuesType">
    <xsd:annotation>
      <xsd:documentation>Sequence of availability status values. Numbers, max. 254, separated with |</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]+(\|[0-9]+)*"/>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
