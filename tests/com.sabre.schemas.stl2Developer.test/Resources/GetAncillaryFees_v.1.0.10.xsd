      <xsd:element maxOccurs="unbounded" minOccurs="0" name="AncillaryFeeData" type="AncillaryFeeData"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ErrorMessage">
    <xsd:annotation>
      <xsd:documentation>Message
          MessageType
                   D = Display
                   W = Warning
                   N = Endorsement
                   R = Restriction
                   E = Error
                   I = Info</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="MessageType" type="STL:UpperCaseAlphaLength1"/>
      <xsd:element minOccurs="0" name="FailCode" type="xsd:int"/>
      <xsd:element minOccurs="0" name="AirlineCode" type="STL:AirlineCode.Merchandising"/>
      <xsd:element minOccurs="0" name="Text" type="STL:Text"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Fee">
    <xsd:simpleContent>
      <xsd:extension base="STL:Amount">
        <xsd:attribute name="Currency" type="STL:CurrencyCodeList"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
